<p id="notice"><%= notice %></p>

<h1>Vagues</h1>

<table>
  <thead>
    <tr>
      <th>Rentree</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vagues.each do |vague| %>
      <tr>
        <td><%= vague.rentree %></td>
        <td><%= vague.status %></td>
        <td><%= link_to 'Show', vague %></td>
        <td><%= link_to 'Edit', edit_vague_path(vague) %></td>
        <td><%= link_to 'Destroy', vague, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vague', new_vague_path %>
